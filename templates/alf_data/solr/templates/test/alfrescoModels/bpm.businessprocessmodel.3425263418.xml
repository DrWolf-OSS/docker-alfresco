<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="bpm:businessprocessmodel">
    <description>Business Process Model</description>
    <author>Alfresco</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/system/1.0" prefix="sys"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
        <import uri="http://www.alfresco.org/model/user/1.0" prefix="usr"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.alfresco.org/model/bpm/1.0" prefix="bpm"/>
    </namespaces>
    <data-types/>
    <constraints>
        <constraint name="bpm:allowedPriority" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>1</value>
                    <value>2</value>
                    <value>3</value>
                </list>
            </parameter>
        </constraint>
        <constraint name="bpm:allowedStatus" type="LIST">
            <parameter name="allowedValues">
                <list>
                    <value>Not Yet Started</value>
                    <value>In Progress</value>
                    <value>On Hold</value>
                    <value>Cancelled</value>
                    <value>Completed</value>
                </list>
            </parameter>
        </constraint>
        <constraint name="bpm:percentage" type="MINMAX">
            <parameter name="minValue">
                <value>0</value>
            </parameter>
            <parameter name="maxValue">
                <value>100</value>
            </parameter>
        </constraint>
        <constraint name="bpm:allowedDescriptionLength" type="LENGTH">
            <parameter name="minLength">
                <value>0</value>
            </parameter>
            <parameter name="maxLength">
                <value>250</value>
            </parameter>
        </constraint>
    </constraints>
    <types>
        <type name="bpm:package">
            <title>Package Folder</title>
            <parent>cm:systemfolder</parent>
            <archive>false</archive>
            <properties/>
            <associations>
                <child-association name="bpm:packageContains">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>sys:base</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                    <duplicate>true</duplicate>
                    <propagateTimestamps>true</propagateTimestamps>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="bpm:task">
            <parent>cm:content</parent>
            <properties>
                <property name="bpm:taskId">
                    <type>d:long</type>
                    <protected>true</protected>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:description">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:startDate">
                    <type>d:date</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:completionDate">
                    <type>d:date</type>
                    <protected>true</protected>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:dueDate">
                    <type>d:date</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:status">
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <default>Not Yet Started</default>
                    <index enabled="true"/>
                    <constraints>
                        <constraint name="bpm:status_anon_33" ref="bpm:allowedStatus"/>
                    </constraints>
                </property>
                <property name="bpm:priority">
                    <type>d:int</type>
                    <mandatory>true</mandatory>
                    <default>2</default>
                    <index enabled="true"/>
                    <constraints>
                        <constraint name="bpm:priority_anon_34" ref="bpm:allowedPriority"/>
                    </constraints>
                </property>
                <property name="bpm:percentComplete">
                    <type>d:int</type>
                    <mandatory>true</mandatory>
                    <default>0</default>
                    <index enabled="true"/>
                    <constraints>
                        <constraint name="bpm:percentComplete_anon_32" ref="bpm:percentage"/>
                    </constraints>
                </property>
                <property name="bpm:comment">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations>
                <association name="bpm:pooledActors">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>sys:base</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects>
                <aspect>cm:ownable</aspect>
            </mandatory-aspects>
        </type>
        <type name="bpm:workflowTask">
            <parent>bpm:task</parent>
            <properties>
                <property name="bpm:context">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:outcome">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:completedItems">
                    <type>d:noderef</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <index enabled="true"/>
                </property>
                <property name="bpm:packageActionGroup">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <default/>
                    <index enabled="true"/>
                </property>
                <property name="bpm:packageItemActionGroup">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <default>read_package_item_actions</default>
                    <index enabled="true"/>
                </property>
                <property name="bpm:hiddenTransitions">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <multiple>true</multiple>
                    <default/>
                    <index enabled="true"/>
                </property>
                <property name="bpm:reassignable">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>true</default>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations>
                <association name="bpm:package">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>bpm:workflowPackage</class>
                        <mandatory enforced="false">true</mandatory>
                        <many>false</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="bpm:activitiOutcomeTask">
            <parent>bpm:workflowTask</parent>
            <properties>
                <property name="bpm:outcomePropertyName">
                    <type>d:qname</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="bpm:startTask">
            <parent>bpm:workflowTask</parent>
            <properties>
                <property name="bpm:workflowDescription">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                    <constraints>
                        <constraint name="bpm:workflowDescription_anon_35" ref="bpm:allowedDescriptionLength"/>
                    </constraints>
                </property>
                <property name="bpm:workflowDueDate">
                    <type>d:date</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:workflowPriority">
                    <type>d:int</type>
                    <mandatory>false</mandatory>
                    <default>2</default>
                    <index enabled="true"/>
                    <constraints>
                        <constraint name="bpm:workflowPriority_anon_36" ref="bpm:allowedPriority"/>
                    </constraints>
                </property>
                <property name="bpm:sendEMailNotifications">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides>
                <property name="bpm:packageActionGroup">
                    <mandatory/>
                    <default>add_package_item_actions</default>
                    <constraints/>
                </property>
                <property name="bpm:packageItemActionGroup">
                    <mandatory/>
                    <default>start_package_item_actions</default>
                    <constraints/>
                </property>
            </overrides>
            <mandatory-aspects/>
        </type>
        <type name="bpm:activitiStartTask">
            <parent>bpm:startTask</parent>
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects>
                <aspect>bpm:endAutomatically</aspect>
            </mandatory-aspects>
        </type>
        <type name="bpm:workflowDefinition">
            <title>Workflow Process Definition</title>
            <parent>cm:content</parent>
            <properties>
                <property name="bpm:definitionName">
                    <title>Workflow Name</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:engineId">
                    <title>Workflow Engine Id</title>
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <default>jbpm</default>
                    <index enabled="true"/>
                </property>
                <property name="bpm:definitionDeployed">
                    <title>Workflow Deployed</title>
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <default>false</default>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
    </types>
    <aspects>
        <aspect name="bpm:assignee">
            <properties/>
            <associations>
                <association name="bpm:assignee">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>cm:person</class>
                        <mandatory enforced="false">true</mandatory>
                        <many>false</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="bpm:assignees">
            <properties/>
            <associations>
                <association name="bpm:assignees">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>cm:person</class>
                        <mandatory enforced="false">true</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="bpm:groupAssignee">
            <properties/>
            <associations>
                <association name="bpm:groupAssignee">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>cm:authorityContainer</class>
                        <mandatory enforced="false">true</mandatory>
                        <many>false</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="bpm:groupAssignees">
            <properties/>
            <associations>
                <association name="bpm:groupAssignees">
                    <source>
                        <mandatory>false</mandatory>
                        <many>false</many>
                    </source>
                    <target>
                        <class>cm:authorityContainer</class>
                        <mandatory enforced="false">true</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="bpm:workflowPackage">
            <properties>
                <property name="bpm:isSystemPackage">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:workflowDefinitionId">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:workflowDefinitionName">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="bpm:workflowInstanceId">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
        <aspect name="bpm:endAutomatically">
            <properties/>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>