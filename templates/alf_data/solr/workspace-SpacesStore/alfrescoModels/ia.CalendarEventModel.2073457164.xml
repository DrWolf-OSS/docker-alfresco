<?xml version="1.0" encoding="UTF-8"?>
<model xmlns="http://www.alfresco.org/model/dictionary/1.0" name="ia:CalendarEventModel">
    <description>Calendar Event Model</description>
    <author>Snig Bhaumik</author>
    <version>1.0</version>
    <imports>
        <import uri="http://www.alfresco.org/model/dictionary/1.0" prefix="d"/>
        <import uri="http://www.alfresco.org/model/content/1.0" prefix="cm"/>
    </imports>
    <namespaces>
        <namespace uri="http://www.alfresco.org/model/calendar" prefix="ia"/>
    </namespaces>
    <data-types/>
    <constraints/>
    <types>
        <type name="ia:ignoreEvent">
            <title>Separate Recurring Event</title>
            <parent>cm:content</parent>
            <properties>
                <property name="ia:date">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="ia:calendarEvent">
            <title>Calendar Event</title>
            <parent>cm:content</parent>
            <properties>
                <property name="ia:whatEvent">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:fromDate">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>BOTH</tokenised>
                    </index>
                </property>
                <property name="ia:toDate">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>BOTH</tokenised>
                    </index>
                </property>
                <property name="ia:whereEvent">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:descriptionEvent">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:colorEvent">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:recurrenceRule">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:recurrenceLastMeeting">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:isOutlook">
                    <type>d:boolean</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:outlookUID">
                    <title>Outlook UID</title>
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <tokenised>FALSE</tokenised>
                    </index>
                </property>
            </properties>
            <associations>
                <child-association name="ia:ignoreEventList">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>ia:ignoreEvent</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </child-association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
        <type name="ia:calendar">
            <title>Calendar Space</title>
            <parent>cm:folder</parent>
            <properties>
                <property name="ia:whatEventDefault">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:fromDateDefault">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>BOTH</tokenised>
                    </index>
                </property>
                <property name="ia:toDateDefault">
                    <type>d:datetime</type>
                    <mandatory>false</mandatory>
                    <index enabled="true">
                        <atomic>true</atomic>
                        <stored>false</stored>
                        <tokenised>BOTH</tokenised>
                    </index>
                </property>
                <property name="ia:whereEventDefault">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
                <property name="ia:colorEventDefault">
                    <type>d:text</type>
                    <mandatory>false</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations>
                <association name="ia:subscribedCalendarList">
                    <source>
                        <mandatory>false</mandatory>
                        <many>true</many>
                    </source>
                    <target>
                        <class>ia:calendar</class>
                        <mandatory enforced="false">false</mandatory>
                        <many>true</many>
                    </target>
                </association>
            </associations>
            <overrides/>
            <mandatory-aspects/>
        </type>
    </types>
    <aspects>
        <aspect name="ia:docFoldered">
            <title>Doc folder</title>
            <properties>
                <property name="ia:docFolder">
                    <title>DocFolder</title>
                    <type>d:text</type>
                    <mandatory>true</mandatory>
                    <index enabled="true"/>
                </property>
            </properties>
            <associations/>
            <overrides/>
            <mandatory-aspects/>
        </aspect>
    </aspects>
</model>